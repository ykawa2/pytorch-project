    "editor.formatOnSave": true,
    "notebook.formatOnSave.enabled": true,
    "files.exclude": {
        ".bash_history": true,
        ".bash_logout": true,
        ".bashrc": true,
        ".cache": true,
        ".config": true,
        ".gitconfig": true,
        ".gnupg": true,
        ".ipython": true,
        ".netrc": true,
        ".profile": true,
        ".python_history": true,
        ".ssh": true,
        ".sudo_as_admin_successful": true,
        ".vscode-server": true,
        "**/__pycache__": true,
        "**/.hypothesis": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true
    },
    "files.trimTrailingWhitespace": true,
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [
        "-sv",
        "--log-cli-level=DEBUG",
        // "--cov=src",
        // "--cov-report=xml",
        // "--cov-report=term",
        "tests"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "flake8.path": [
        "/home/docker/.local/bin/pflake8"
    ],
    "mypy-type-checker.path": [
        "/home/docker/.local/bin/mypy"
    ],
    // isort settings
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
}